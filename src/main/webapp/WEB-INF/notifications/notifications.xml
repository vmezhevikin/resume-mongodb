<!DOCTYPE xml>
<beans>
	<bean id="restoreAccessNotification" class="net.devstudy.resume.model.NotificationMessage">
		<property name="subject" value="Restore Access" />
		<property name="content">
			<value>restore-access.html</value>
		</property>
	</bean>
	<bean id="passwordChangedNotification" class="net.devstudy.resume.model.NotificationMessage">
		<property name="subject" value="Password changed" />
		<property name="content">
			<value>password-changed.html</value>
		</property>
	</bean>
	<bean id="confirmRegistrationNotification" class="net.devstudy.resume.model.NotificationMessage">
		<property name="subject" value="Welcome to MyResume" />
		<property name="content">
			<value>confirm-registration.html</value>
		</property>
	</bean>
	<bean id="returnReminderNotification" class="net.devstudy.resume.model.NotificationMessage">
		<property name="subject" value="Come back to MyResume" />
		<property name="content">
			<value>return-reminder.html</value>
		</property>
	</bean>
	<bean id="confirmEmailNotification" class="net.devstudy.resume.model.NotificationMessage">
		<property name="subject" value="Confirm email" />
		<property name="content">
			<value>confirm-email.html</value>
		</property>
